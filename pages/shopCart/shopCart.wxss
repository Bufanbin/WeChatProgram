.app {
    position: fixed;
    width: 100%;
    height: 100%;
}
.shop-cart-heat {
    height: 85rpx;
    line-height: 85rpx;
    text-align: center;
    overflow: hidden;
    position: relative;
}
.seleceAll {
    float: left;
    margin-left: 25rpx;
    line-height: 100rpx;
}
.deletebtn {
    display: block;
    width: 100rpx;
    height: 100%;
    float: left;
    color: #f25454;
    font-size: 32rpx;
}
.editbtn {
    display: block;
    width: 110rpx;
    height: 100%;
    float: right;
    color: #4c93dc;
    font-size: 30rpx;
}
.shop-cart-list {
    height: calc(100% - 205rpx);
    overflow-y: auto;
}
.shop-cart-item {
    display: inline-block;
    background: #f6f6f6;
    width: 100%;
    overflow: hidden;
}
.seleceOne {
    float: left;
    line-height: 226rpx;
}
.newsp_shop_info > image {
    width: 196rpx;
    height: 196rpx;
}
.shop-goods-info {
    float: right;
    position: relative;
    width: calc(100% - 272rpx);
    margin-left: 10rpx;
    height: 100%;
}
.shop-goods-info > text {
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    display: block;
}
.goods-name {
    margin-top: 20rpx;
    color: #000;
    font-size: 30rpx;
}
.goods-spec {
    font-size: 28rpx;
    color: #888;
    margin-top: 20rpx;
}
.goods-price {
    font-size: 36rpx;
    color: #fe5d5d;
    margin-top: 30rpx;
}
.goods-dismiss {
    font-size: 26rpx;
    color: #fff;
    margin-top: 30rpx;
    background: #959595;
    width: 90rpx;
    text-align: center;
    padding: 5rpx 0;
}
.goods-num {
    color: #888;
    font-size: 30rpx;
    float: right;
    position: absolute;
    top: 76rpx;
    right: 40rpx;
}
.goods-del-miss {
    color: #3875c0;
    font-size: 30rpx;
    float: right;
    position: absolute;
    top: 110rpx;
    right: 15rpx;
    width: 155rpx;
    height: 60rpx;
    line-height: 60rpx;
    text-align: center;
    border: 1px solid #3875c0;
    border-radius: 5rpx;
}
.goods-delete {
    position: absolute;
    width: 100rpx;
    height: 226rpx;
    line-height: 240rpx;
    color: #fff;
    background: #f77;
    text-align: center;
    right: 0;
    font-size: 30rpx;
    top: 0;
}
.edit-operate {
    overflow: hidden;
    margin-top: 35rpx;
}
.edit-operate > button {
    width: 50rpx;
    height: 50rpx;
    line-height: 50rpx;
    border: 0;
    color: #838383;
    padding: 0;
    float: left;
    background: #f6f6f6;
    font-size: 44rpx;
}
.edit-operate > input {
    float: left;
    height: 50rpx;
    text-align: center;
    width: 130rpx;
    border-left: 1px solid #fff;
    border-right: 1px solid #fff;
    min-height: auto;
    color: #838383;
    font-size: 34rpx;
}
.result-show {
    height: 120rpx;
}
.statement {
    width: 305rpx;
    line-height: 120rpx;
}
.cart-balance {
    width: 445rpx;
    line-height: 120rpx;
    color: #353535;
}
.cart-balance > text {
    float: right;
}
.total-money {
    margin-right: 50rpx;
    color: #ff6060;
    font-size: 36rpx;
}
.express-fee {
    margin-right: 20rpx;
    font-size: 28rpx;
    color: #888;
}
.shop-nothing > view {
    font-size: 150rpx;
    color: #fff;
    background: #cbcbcb;
    width: 300rpx;
    height: 300rpx;
    position: relative;
    text-align: center;
    line-height: 300rpx;
    border-radius: 100%;
    margin-left: 225rpx;
    margin-top: 310rpx;
}
.shop-nothing > text {
    display: block;
    text-align: center;
    color: #515151;
    font-size: 38rpx;
    margin-top: 45rpx;
}
.overdued {
    width: 18px;
    display: inline-block;
    float: left;
    font-size: 24rpx;
    color: #fe5d5d;
    position: relative;
    top: 50%;
    transform: translateY(-50%);
}
.operate {
    float: left;
    margin-left: 15rpx;
}
.canclebtn {
    color: #9e9e9e;
}
.newsp_shop_name {
    width: 100%;
    height: 94rpx;
    line-height: 94rpx;
    background: #fff;
    font-size: 30rpx;
    color: #888;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
    padding: 0 23rpx;
    box-sizing: border-box;
}
.newsp_shop_title {
    color: #353535;
}
.newsp_shop_info {
    width: 100%;
    height: 226rpx;
    padding: 0 23rpx;
    box-sizing: border-box;
}
.shop-goods-img {
    position: relative;
    width: 196rpx;
    height: 196rpx;
    margin: 15rpx;
    float: left;
}
.shop_goods_icon {
    width: 50rpx;
    height: 50rpx;
    background: rgba(0, 0, 0, 0.6);
    display: flex;
    align-items: center;
    justify-content: center;
    color: #fff;
    position: absolute;
    bottom: 0;
    right: 0;
}
.icon-naozhong, .icon-fenxiao {
    font-size: 33rpx;
}
.goods-spec {
    width: 280rpx;
}