.app {
    position: fixed;
    width: 100%;
    height: 100%;
    top: 0;
    left: 0;
}
.dis-order-top {
    height: 90rpx;
    border-bottom: 2rpx solid #e5e5e5;
    box-sizing: border-box;
}
.dis-order-item {
    border-bottom: 10rpx solid #ececec;
    overflow: hidden;
}
.dis-order-item:last-child {
    margin-bottom: 50rpx;
}
.dis-item-top {
    margin: 0 20rpx;
    border-bottom: 1rpx solid #eee;
    overflow: hidden;
    padding: 25rpx 0;
}
.dis-pic {
    width: 190rpx;
    height: 190rpx;
    margin-left: 10rpx;
    float: left;
}
.dis-order-info {
    float: left;
    margin-left: 25rpx;
    width: 470rpx;
}
.dis-name {
    color: #363636;
    font-size: 30rpx;
    line-height: 30rpx;
    margin-top: 20rpx;
    width: 100%;
}
.dis-classify {
    color: #9e9e9e;
    font-size: 28rpx;
    line-height: 28rpx;
    margin-top: 20rpx;
}
.dis-type {
    width: 140rpx;
    height: 72rpx;
    line-height: 72rpx;
    text-align: center;
    color: #4099eb;
    border-radius: 5rpx;
    display: inline-block;
    float: right;
}
.finish.dis-type {
    color: #ff6b6b;
}
.orange.dis-type {
    color: #ff9b6e;
}
.blown.dis-type {
    color: #6b6b6b;
}
.set-ratio-box {
    height: 150rpx;
    margin-top: 10rpx;
    text-align: center;
    overflow: hidden;
}
.set-ratio-item {
    width: 150rpx;
    margin-left: 120rpx;
    float: left;
    color: #545151;
    font-size: 30rpx;
}
.set-ratio-item:first-child {
    margin-left: 0rpx;
}
.set-title {
    line-height: 30rpx;
    margin-top: 15rpx;
}
.set-num {
    line-height: 30rpx;
    margin-top: 30rpx;
}
.pay-time {
    margin-left: 30rpx;
    font-size: 30rpx;
    color: #b3b3b3;
    padding-bottom: 30rpx;
    line-height: 30rpx;
}
::-webkit-scrollbar {
    width: 0;
    height: 0;
}