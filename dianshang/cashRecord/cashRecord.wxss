page {
    overflow: hidden;
    width: 100%;
    height: 100%;
    position: fixed;
    background: #f0f0f0;
}
.cashRec_top {
    width: 100%;
    background: #fff;
    color: #888;
    font-size: 28rpx;
    overflow: hidden;
}
.cashRec_top > view {
    width: 25%;
    text-align: center;
    height: 110rpx;
    padding-top: 20rpx;
    border-bottom: 2rpx solid #fff;
    box-sizing: border-box;
    float: left;
}
.cashRec_top >view.select {
    color: #f02437;
    border-bottom: 2rpx solid #f02437;
}
.cashlist_box {
    color: #212121;
    font-size: 24rpx;
    text-align: center;
    width: 100%;
    height: calc(100% - 110rpx);
}
.cashbox {
    background: #fff;
    padding: 0 25rpx;
    margin-top: 20rpx;
    width: 100%;
    overflow: hidden;
    box-sizing: border-box;
}
.cashtop {
    height: 65rpx;
    line-height: 65rpx;
    border-bottom: 2rpx solid #f0f0f0;
}
.cash_orderid {
    float: left;
    color: #888;
}
.cash_addtime {
    float: left;
    color: #888;
    margin-left: 55rpx;
}
.cash_type {
    float: right;
}
.color0 {
    color: #4541e6;
}
.color1 {
    color: #09bb07;
}
.color2 {
    color: #f02437;
}
.cashbody {
    text-align: left;
    overflow: hidden;
    padding-bottom: 25rpx;
}
.cashbody_left {
    float: left;
    width: 325rpx;
}
.cashbody_right {
    float: right;
    width: 325rpx;
}
.cashbody_left view, .cashbody_right view {
    white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
    padding-top: 25rpx;
}