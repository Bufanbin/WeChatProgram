page {
    background: #eee;
}
.app {
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 0;
    overflow: hidden;
}
.goods-name {
    font-size: 28rpx;
    color: #000;
    font-weight: bold;
    width: 100%;
    display: inline-block;
    word-break: break-all;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: pre-wrap;
}
.goods-price {
    display: inline-block;
    text-decoration: line-through;
}
.goods-line {
    display: block;
    height: auto;
    padding: 0 28rpx 30rpx;
    background: #fff;
    box-sizing: border-box;
}
.right-arrow {
    position: absolute;
    right: 45rpx;
    float: left;
    color: #727171;
}
.evaluate-head {
    background-color: #f4f4f4;
    height: 65rpx;
    line-height: 65rpx;
    text-align: center;
    position: relative;
    width: 100%;
    display: inline-block;
}
.evaluate-title {
    position: relative;
    top: 4rpx;
}
.wxParse {
    padding: 30rpx 18rpx 140rpx;
}
.goods-bottom {
    z-index: 5;
}
.goods-suspension {
    width: 116rpx;
}
.goods-service-text {
    height: 30rpx;
    line-height: 40rpx;
}
.choose-spec {
    text-overflow: ellipsis;
    overflow: hidden;
    white-space: nowrap;
}
.goods-spec {
    height: 445rpx;
}
.evaluate-info {
    line-height: 70rpx;
}
.icon-gouwuche {
    position: absolute;
    right: 30rpx;
    top: 15rpx;
    font-size: 30rpx;
    z-index: 9999;
    background: rgba(0, 0, 0, 0.5);
    border-radius: 100%;
    width: 60rpx;
    height: 60rpx;
    line-height: 60rpx;
    color: #fff;
    text-align: center;
}
.icon-09handbag {
    background: #dbdbdb;
    color: #fff;
    font-size: 100rpx;
    width: 230rpx;
    height: 230rpx;
    line-height: 230rpx;
    border-radius: 100%;
    text-align: center;
    margin: 140rpx auto 0;
}
.nothing-page > button {
    width: 419rpx;
    height: 97rpx;
    line-height: 97rpx;
    font-size: 36rpx;
}
.dist_top_div {
    width: 178rpx;
    height: 74rpx;
    line-height: 74rpx;
    text-align: center;
    color: #fff;
    font-size: 28rpx;
    opacity: 0.8;
    padding: 0;
    border-radius: 0;
}
.dist_btn_box {
    position: absolute;
    right: 0;
    top: 50%;
    transform: translateY(-50%);
    -webkit-transform: translateY(-50%);
}
.swiper_box {
    position: relative;
    background: #fff;
}
.addShopCar {
    width: 50%;
}
.paynow {
    width: 50%;
}
.newdist-commission-box {
    width: 100%;
    padding: 45rpx 28rpx;
    margin-top: 20rpx;
    background: #fff;
    box-sizing: border-box;
}
.dist-commission-left {
    float: left;
    font-size: 28rpx;
    color: #888;
}
.goods-head-content, .detail_title_box, .empty-height, .sp_comment_user,
.newsp_parameter_box, .sp_comment_details, .goods-recommend {
    background: #fff;
}
.detail_title_box {
    margin-top: 20rpx;
}
.mycanvas {
    width: 640px;
    height: 1008px;
    background: #f00;
    opacity: 0;
    position: fixed;
    top: 0;
    left: 1000000000000px;
}
.distribute-box {
    width: 100%;
    height: calc(100% - 98rpx);
}
.strore-box {
    background: #fff;
    border: 0;
    margin-bottom: 0;
    margin-top: 20rpx;
}
.store-right {
    float: left;
}
.newtype, .newtype1 {
    width: auto;
}
.newtype1 {
    margin-left: 28px;
}
.newdis-btn {
    margin-top: -25rpx;
}
.sp_bottom_store1 {
    width: calc(100% - 232rpx);
}