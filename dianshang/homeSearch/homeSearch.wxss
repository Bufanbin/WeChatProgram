page {
    background: #ececec;
}
.app {
    position: fixed;
    width: 100%;
    height: 100%;
    overflow: hidden;
    left: 0;
    top: 0;
}
.goods-search {
    height: 116rpx;
    padding-top: 26rpx;
}
.goods-search-input {
    height: 84rpx;
    line-height: 84rpx;
}
.search-icon {
    float: left;
}
.goods-search-input > input {
    float: left;
    min-width: calc(100% - 194rpx);
    max-width: calc(100% - 194rpx);
    height: 84rpx;
    font-size: 28rpx;
    color: #353535;
}
.goods-search-place {
    color: #cbcbcb !important;
}
.home-search-btn {
    right: 0 !important;
    width: 144rpx;
    height: 84rpx;
    line-height: 84rpx;
    text-align: center;
    background: #8837d2 !important;
    color: #fff;
    font-size: 28rpx;
}
.icon-classification:before {
    font-size: 46rpx;
}
.home-goods-box {
    width: 100%;
    margin-top: 21rpx;
    background: #ececec;
}
.home-goods-head {
    width: 100%;
    height: 82rpx;
    line-height: 82rpx;
    padding: 0 21rpx;
    background: #fff;
    border-bottom: 2rpx solid #dcdcdc;
    box-sizing: border-box;
}
.home-goods-left {
    float: left;
    font-size: 30rpx;
    color: #353535;
}
.home-goods-right {
    float: right;
    font-size: 47rpx;
    color: #9a9a9a;
    line-height: 82rpx;
}
.home-goods-item {
    width: 100%;
    padding: 28rpx 37rpx 30rpx 21rpx;
    background: #fff;
    border-bottom: 2rpx solid #dfdfdf;
    box-sizing: border-box;
}
.home-goods-item:last-child {
    border-bottom: 0;
}
.home-goods-img {
    width: 166rpx;
    height: 166rpx;
    float: left;
}
.home-goods-info {
    float: left;
    width: calc(100% - 187rpx);
    margin-left: 21rpx;
    position: relative;
}
.home-goods-name {
    font-size: 30rpx;
    color: #000;
    display: -webkit-box;
    word-break: break-all;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
    overflow: hidden;
    text-overflow: ellipsis;
}
.home-goods-price {
    font-size: 36rpx;
    color: #e64340;
    margin-top: 5rpx;
}
.home-goods-sale {
    font-size: 28rpx;
    color: #888;
    margin-top: 5rpx;
}
.home-group-btn {
    width: 124rpx;
    height: 60rpx;
    line-height: 60rpx;
    text-align: center;
    font-size: 26rpx;
    color: #fff;
    background: #8837d2;
    border-radius: 9rpx;
    cursor: pointer;
    position: absolute;
    right: 0;
    bottom: 14rpx;
}
.newseckill-oldprice {
    font-size: 28rpx;
    color: #c3c3c3;
}
.home-seck-progress {
    width: 316rpx;
    height: 14rpx;
}
.home-progress-span {
    color: #221334;
    font-size: 30rpx;
    margin-left: 19rpx;
}
.home-seckill-deadline > text {
    color: #747474;
}
.home-seckill-deadline .seckill-time {
    padding: 2rpx 14rpx;
    background: #f76d6d;
}
.home-goods-scroll1 {
    width: 100%;
    height: calc(100% - 167rpx);
}
.order_list {
    height: 86rpx;
    font-size: 32rpx;
    border-bottom: 2rpx solid #dcdcdc;
}
.order_list .order_item {
    height: 86rpx;
    line-height: 86rpx;
}
.home-goods-scroll2 {
    width: 100%;
    height: calc(100% - 86rpx);
    background: #ececec;
}
.order_checked {
    color: #8837d2 !important;
}
.order_up.select, .order_down.select {
    color: #8837d2;
}
.home-dist-price {
    float: left;
    line-height: 60rpx;
}
.home-searchdis-btn {
    float: right;
    width: 138rpx;
    height: 60rpx;
    line-height: 60rpx;
    text-align: center;
    font-size: 26rpx;
    color: #fff;
    background: #8837d2;
    border-radius: 10rpx;
}
.home-dist-sale {
    float: left;
}
.home-dist-commision {
    float: right;
    font-size: 24rpx;
    color: #c2c2c2;
    margin-top: 6rpx;
}
.newseckill-span {
    font-size: 26rpx;
    color: #211233;
}