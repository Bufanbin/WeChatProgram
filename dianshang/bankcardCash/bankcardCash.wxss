page {
    background: #f0f0f0;
}
.app {
    width: 100%;
    height: 100%;
    overflow: hidden;
}
.withdrawal-head-box {
    width: 100%;
    background: #fff;
}
.withdrawal-title {
    width: 100%;
    height: 90rpx;
    line-height: 90rpx;
    padding-left: 30rpx;
    font-size: 28rpx;
    color: #000;
    background: #fff;
    box-sizing: border-box;
}
.withdrawal-money {
    width: 100%;
    height: 145rpx;
    line-height: 145rpx;
    text-align: center;
    font-size: 60rpx;
    color: #ef2437;
    background: #fff;
    box-sizing: border-box;
}
.withdrawal-account-box {
    width: 100%;
    margin-top: 20rpx;
}
.withdrawal-account-item {
    width: 100%;
    padding: 29rpx;
    background: #fff;
    border-bottom: 2rpx solid #f0f0f0;
    box-sizing: border-box;
}
.withdrawal-account-item:last-child {
    border-bottom: 0;
}
.withdrawal-account-left {
    float: left;
    font-size: 30rpx;
    color: #353535;
}
.withdrawal-account-right {
    float: right;
    font-size: 30rpx;
    color: #353535;
}
.withdrawal-input {
    display: inline-block;
    vertical-align: middle;
    font-size: 30rpx;
    color: #353535;
    text-align: right;
    outline: none;
}
.withdrawal-place {
    font-size: 30rpx;
    color: #888;
}
.withdrawal-btn {
    width: 691rpx;
    height: 94rpx;
    line-height: 94rpx;
    text-align: center;
    padding: 0;
    font-size: 30rpx;
    color: #fff;
    margin: 0 auto;
    background: #ef2437;
    border-radius: 10rpx;
}
.withdrawal-tip-box {
    width: 600rpx;
    height: 99rpx;
    line-height: 99rpx;
    text-align: center;
    font-size: 30rpx;
    color: #fff;
    background: rgba(0, 0, 0, 0.7);
    border-radius: 3rpx;
    position: fixed;
    top: 30%;
    left: 50%;
    transform: translateX(-50%);
    z-index: 10;
}
.withdrawal-account-text {
    color: #888;
}
.bankcard-tip-box {
    width: 100%;
    padding: 35rpx 30rpx;
    background: #f0f0f0;
    box-sizing: border-box;
}
.bankcard-tip {
    font-size: 28rpx;
    color: #888;
}
.icon-rightarrow {
    font-size: 40rpx;
    margin-left: 10rpx;
    position: relative;
    top: 4rpx;
    color: #c0c0c0;
}
.hidden {
    display: none !important;
}