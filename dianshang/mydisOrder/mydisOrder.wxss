page {
    background: #fff;
}
.app {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
}
.orderType {
    height: 80rpx;
    line-height: 80rpx;
    border-bottom: 2rpx solid #e5e5e5;
    box-sizing: border-box;
}
.orderType > view {
    font-weight: normal;
}
.orderType > view.select {
    color: #ef2437;
    border-bottom: 5rpx solid #ef2437;
    box-sizing: border-box;
}
.order-item {
    position: relative;
    border-bottom: 2rpx solid #eee;
    min-height: 305rpx;
    padding-top: 30rpx;
}
.order-item:first-child {
    padding-top: 0;
}
.order-item:last-child {
    border-bottom: 0;
    margin-bottom: 30rpx;
}
.order-item-info {
    margin-bottom: 15rpx;
}
.order-goods-info {
    width: 473rpx;
    margin-left: 22rpx;
}
.order-goods-name {
    margin-top: 15rpx;
    color: #000;
    font-size: 30rpx;
}
.order-goods-spec {
    font-size: 28rpx;
    color: #888;
    float: left;
    margin-top: 0;
}
.order-goods-price {
    font-size: 30rpx;
    margin-top: 25rpx;
    color: #ef2437;
}
.order-goods-num {
    color: #888;
    font-size: 30rpx;
    position: relative;
    top: 4rpx;
}
.order-item-subtotal {
    text-align: right;
    padding-right: 15rpx;
}
.order-item-subtotal > text {
    margin-right: 25rpx;
    font-size: 30rpx;
}
.order-operate > button {
    height: 70rpx;
    line-height: 70rpx;
    border: 1px solid #a0a0a0;
    color: #585858;
    font-size: 30rpx;
    box-sizing: unset;
    background: #fff;
    margin-bottom: 20rpx;
    float: right;
}
.order-operate > button.style1 {
    color: #54b5e3;
    border-color: #54b5e3;
}
.order-operate > button.style2 {
    color: #fc7d37;
    border-color: #fc7d37;
}
.receipt-info {
    font-size: 24rpx;
}
.receipt-close {
    height: 50rpx;
    width: 160rpx;
    line-height: 50rpx;
    font-size: 24rpx;
}
.newsp_shop_name {
    width: 100%;
    height: 94rpx;
    line-height: 94rpx;
    background: #fff;
    font-size: 30rpx;
    color: #888;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
    padding: 0 23rpx 0 45rpx;
    box-sizing: border-box;
}
.newsp_shop_title {
    color: #353535;
}
.order-goods-info > text {
    max-width: 62%;
}
.order-item-info > image {
    width: 198rpx;
    height: 198rpx;
}
.order-goods-info .order-goods-name {
    display: -webkit-box;
    word-break: break-all;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: normal;
}
.subtotal-num {
    color: #e64240;
}
.trade-type {
    top: 18rpx;
}
.trade-type.type2 {
    color: #ef2437;
}
.order-goods-line {
    width: 100%;
    margin-top: 15rpx;
}
.withdrawal-tip-box {
    width: 562rpx;
    padding: 20rpx 10rpx;
    text-align: center;
    font-size: 30rpx;
    color: #fff;
    background: rgba(0, 0, 0, 0.7);
    border-radius: 3rpx;
    position: fixed;
    top: 35%;
    left: 50%;
    transform: translateX(-50%);
    z-index: 10;
}