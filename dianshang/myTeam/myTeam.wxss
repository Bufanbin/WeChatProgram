.teamer image {
    width: 120rpx;
    height: 120rpx;
    border-radius: 8rpx;
    margin: 19rpx;
}
.teamer>view {
    display: inline-block;
    vertical-align: 100%;
}
.teamer>view view {
    margin: 10rpx 0;
}
.teamer {
    border-bottom: 1px solid #cecece;
}
.team_num {
    line-height: 80rpx;
    height: 80rpx;
    border-bottom: 1px solid #cecece;
}
.team_num text {
    margin-left: 20rpx;
}