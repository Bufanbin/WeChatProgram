page {
    background-color: #ececec;
}
.classify {
    width: 100%;
    height: 89rpx;
    border-bottom: 1px solid #ececec;
    line-height: 89rpx;
    text-indent: 20rpx;
    font-size: 30rpx;
    background-color: #fff;
}
.icon-rightarrow1 {
    float: right;
    margin-right: 20rpx;
}
.icon-bottomarrow {
    float: right;
    margin-right: 20rpx;
}
.classify_ordinary {
    width: 100%;
    overflow: hidden;
    padding: 20rpx 0 20rpx 0;
    background-color: #fff;
    margin-bottom: 13rpx;
}
.ordinary {
    width: 159rpx;
    height: 68rpx;
    line-height: 68rpx;
    float: left;
    font-size: 28rpx;
    color: #888;
    text-align: left;
    margin-left: 28rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}