page {
    overflow: hidden;
    width: 100%;
    height: 100%;
    position: fixed;
    background: #f0f0f0;
}
.ragedetail_top {
    width: 100%;
    background: #fff;
    color: #888;
    font-size: 30rpx;
    overflow: hidden;
    margin-bottom: 20rpx;
}
.ragedetail_top > view {
    width: 33.33%;
    text-align: center;
    height: 100rpx;
    line-height: 97rpx;
    border-bottom: 3rpx solid #fff;
    box-sizing: border-box;
    float: left;
}
.ragedetail_top >view.select {
    color: #f02437;
    border-bottom: 3rpx solid #f02437;
}
.ragelist_tit {
    width: 100%;
    background: #fff;
    color: #353535;
    font-size: 28rpx;
    text-align: center;
    overflow: hidden;
    margin-bottom: 3rpx;
}
.ragelist_tit > view {
    float: left;
    width: 25%;
    height: 90rpx;
    line-height: 90rpx;
}
.ragelist_box {
    color: #070707;
    font-size: 28rpx;
    text-align: center;
    width: 100%;
    height: calc(100% - 213rpx);
}
.rageitem_box {
    width: 100%;
    overflow: hidden;
    background: #fff;
    margin-bottom: 1px;
}
.rageitem_box > view {
    width: 25%;
    height: 90rpx;
    line-height: 90rpx;
    float: left;
}
.rage_money {
    color: #f02437;
}
.rage_type1, .rage_type0 {
    color: #c2c2c2;
}
.rage_type3 {
    color: #09bb07;
}